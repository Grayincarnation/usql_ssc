USE DATABASE UkPostcodes;
USE SCHEMA Postcodes;

CREATE TABLE IF NOT EXISTS Postcodes_Range 
( 
Postcode string, 
CountyCode string, 
DistrictCode string, 
CountryCode string, 
Latitude decimal?, 
Longitude decimal?, 
INDEX idx_Postcodes CLUSTERED(Postcode) 
PARTITIONED BY RANGE(Latitude, Longitude));