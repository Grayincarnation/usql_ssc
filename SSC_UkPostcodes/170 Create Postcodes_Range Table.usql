USE DATABASE UkPostcodes;
USE SCHEMA Postcodes;

DROP TABLE IF EXISTS Postcodes_Range;

CREATE TABLE IF NOT EXISTS Postcodes_Range 
( 
Postcode string, 
CountyCode string, 
DistrictCode string, 
CountryCode string, 
Latitude decimal?, 
Longitude decimal?, 
INDEX idx_Postcodes CLUSTERED(Postcode) 
DISTRIBUTED BY RANGE(Latitude, Longitude));